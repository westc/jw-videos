{
  "name": "jw-videos",
  "productName": "JW Videos",
  "version": "1.0.0",
  "description": "Show jw.org videos while participating in public witnessing.",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager .",
    "build-mac64": "electron-packager . --platform=darwin --arch=x64 --prune=true --out=JW-Videos_For_Mac64 --overwrite --ignore=\"JW-Videos_For.*|\\.gitignore\" --icon=\"assets/icons/JWVideos-512.png.icns\"",
    "build-win64": "electron-packager . --platform=win32 --arch=x64 --prune=true --out=JW-Videos_For_Win64 --overwrite --ignore=\"JW-Videos_For.*|\\.gitignore\" --icon=\"assets/icons/JWVideos-256.png.ico\""
  },
  "repository": "https://github.com/westc/jw-videos",
  "author": "Christopher West (http://cwestify.com/)",
  "license": "ISC",
  "devDependencies": {
    "electron-packager": "^8.7.0",
    "electron": "^1.6.6"
  }
}
